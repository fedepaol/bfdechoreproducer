log file /etc/frr/frr.log informational
log timestamp precision 3
hostname dummyhostname
ip nht resolve-via-default
ipv6 nht resolve-via-default

router bgp 100
  no bgp ebgp-requires-policy
  no bgp network import-check
  no bgp default ipv4-unicast

  bgp router-id 10.111.221.11
  neighbor 10.111.221.21 remote-as 200
  neighbor 10.111.221.21 ebgp-multihop
  neighbor 10.111.221.21 port 179
  neighbor 10.111.221.21 timers 2 1
  neighbor 10.111.221.21 password password
  neighbor 10.111.221.21 update-source 10.1.1.254
  neighbor 10.111.221.21 bfd profile foo

  address-family ipv4 unicast
    neighbor 10.111.221.21 activate
    neighbor 10.111.221.21 route-map 10.111.221.21-in in
    neighbor 10.111.221.21 route-map 10.111.221.21-out out
  exit-address-family
  address-family ipv6 unicast
    neighbor 10.111.221.21 activate
    neighbor 10.111.221.21 route-map 10.111.221.21-in in
    neighbor 10.111.221.21 route-map 10.111.221.21-out out
  exit-address-family

bfd
  profile bar
    echo-interval 90
